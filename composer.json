{
  "name": "phpassert/core",
  "description": "The core of phpassert",
  "minimum-stability": "alpha",
  "license": "MIT",
  "scripts": {
    "test": "php vendor/bin/phpunit",
    "test:coverage": "phpdbg -qrr ./vendor/bin/phpunit --coverage-html coverage"
  },
  "autoload": {
    "psr-4": {
      "PHPAssert\\Core\\": "src/PHPAssert/Core/",
      "unit\\PHPAssert\\Core\\": "tests/unit//PHPAssert/Core/"
    },
    "files": ["src/PHPAssert/Core/Utils/Test.php"]
  },
  "authors": [
    {
      "name": "Thomas Farla",
      "email": "thomas@farla.io"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "anahkiasen/underscore-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.*",
    "mikey179/vfsStream": "1.6.x-dev"
  }
}
